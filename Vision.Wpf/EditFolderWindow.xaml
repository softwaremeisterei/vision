<Window x:Class="Vision.Wpf.EditFolderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Edit folder" Width="240.196" Height="156.395" Loaded="Window_Loaded">
    <StackPanel>
        <Label Content="Name" />
        <TextBox Name="tbName" Text="{Binding Folder.Name}" />
        <Button Content="OK" Click="BtnOK_Click" IsDefault="True" Margin="0,10,0,0" />
    </StackPanel>
</Window>
