<Window x:Class="Vision.Wpf.PromptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Prompt" Width="240.196" Height="104.365" Loaded="Window_Loaded">
    <StackPanel>
        <TextBlock x:Name="tbHint" Text="Enter some text" />
        <TextBox x:Name="tbResponse" />
        <Button Content="OK" Click="BtnOK_Click" IsDefault="True" Margin="0,10,0,0" />
    </StackPanel>
</Window>
